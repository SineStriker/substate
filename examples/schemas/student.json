{
    "namespace": "music",
    "dllexport": "SAMPLE_EXPORT",
    "include": {
        "QByteArray": "QtCore/QByteArray",
        "QString": "QtCore/QString"
    },
    "classes": [
        {
            "name": "Root",
            "properties": {
                "version": {
                    "type": "QByteArray",
                    "default": "\"0.0.0.1\""
                },
                "author": {
                    "type": "QString",
                    "default": "\"you-know-who\""
                },
                "tracks": "Substate::VectorNode<Track>"
            },
            "fileName": "root.h"
        },
        {
            "name": "Track",
            "reserved": true,
            "enums": [
                {
                    "name": "Type",
                    "values": [
                        "Voice",
                        "BGM"
                    ]
                }
            ],
            "ctor": {
                "args": [
                    {
                        "type": "Type",
                        "name": "type",
                        "member": "type"
                    }
                ]
            },
            "properties": {
                "name": "QString",
                "volume": {
                    "type": "double",
                    "default": 0
                }
            },
            "fileName": "track.h"
        },
        {
            "name": "VoiceTrack",
            "super": "Track",
            "enums": [
                {
                    "name": "Gender",
                    "values": [
                        "Male",
                        "Female"
                    ]
                }
            ],
            "ctor": {
                "super": [
                    "Voice"
                ]
            },
            "properties": {
                "gender": {
                    "type": "Gender",
                    "default": "VoiceTrack::Male"
                },
                "curves": "Substate::BytesNode<Int16>",
                "notes": "Substate::SheetNode<Note>"
            },
            "fileName": "track.h"
        },
        {
            "name": "BGMTrack",
            "super": "Track",
            "ctor-d": {
                "super": [
                    "BGM"
                ]
            },
            "properties": {
                "path": "QString"
            },
            "fileName": "track.h"
        },
        {
            "name": "Note",
            "properties": {
                "lyric": "QString",
                "keyNum": {
                    "type": "int",
                    "default": 60
                }
            },
            "fileName": "note.h"
        }
    ]
}