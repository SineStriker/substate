{
    "include": {
        "std::string": "string",
        "QString": "QtCore/QString"
    },
    "namespace": "music",
    "classes": {
        "Root": {
            "properties": {
                "version": {
                    "type": "std::string",
                    "default": "\"0.0.0.1\""
                },
                "author": {
                    "type": "QString",
                    "default": "\"you-know-who\""
                },
                "tracks": "Substate::VectorNode<Track>"
            },
            "fileName": "root.h"
        },
        "Track": {
            "enums": {
                "Type": [
                    "Voice",
                    "BGM"
                ]
            },
            "ctor": null,
            "ctor-d": {
                "args": [
                    {
                        "type": "Type",
                        "token": "type",
                        "member": "type"
                    }
                ]
            },
            "properties": {
                "name": "QString",
                "volume": {
                    "type": "double",
                    "default": 0
                }
            },
            "fileName": "track.h"
        },
        "VoiceTrack": {
            "super": "Track",
            "enums": {
                "Gender": [
                    "Male",
                    "Female"
                ]
            },
            "ctor-d": {
                "super": [
                    "Track"
                ]
            },
            "properties": {
                "gender": {
                    "type": "Gender",
                    "default": "VoiceTrack::Male"
                },
                "curves": "Substate::BytesNode<int16_t>",
                "notes": "Substate::SheetNode<Note>"
            },
            "fileName": "track.h"
        },
        "Note": {
            "properties": {
                "lyric": "QString",
                "keyNum": {
                    "type": "int",
                    "default": 60
                }
            },
            "fileName": "note.h"
        }
    }
}